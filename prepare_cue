# leaf1  

- hosts: leaf1
  name: disable NCLU
  become: yes
  gather_facts: no
  tasks:
    - name: disable NCLU
      shell: sudo systemctl stop netd
      shell: sudo systemctl disable netd
    - name: copy CUE
      copy: 
        src: /home/cumulus/ON-10/cue-test.deb 
        dest: /home/cumulus/cue-test.deb
    - name: install CUE
      apt:
        deb: /home/cumulus/cue-test.deb
     - name: enable CUE
      shell: sudo systemctl enable cued 
      shell: sudo systemctl start cued   
